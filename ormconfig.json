{
  "type": "mongodb",
  "url": "mongodb://mongodb/myappdb",
  "synchronize": true,
  "logging": true,
  "entities": ["src/entity/**/*"],
  "migrations": ["src/migration/**/*"],
  "subscribers": ["src/subscriber/**/*"],
  "useNewUrlParser": true,
  "cli": {
    "entitiesDir": "src/entity",
    "migrationsDir": "src/migration",
    "subscribersDir": "src/subscriber"
  }
}
